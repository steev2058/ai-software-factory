{
  "name": "Agent-Analytics-6h",
  "nodes": [
    {
      "id": "cron1",
      "name": "Every 6h",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.2,
      "position": [260, 260],
      "parameters": {
        "rule": { "interval": [ { "field": "cronExpression", "expression": "0 */6 * * *" } ] }
      }
    },
    {
      "id": "exec1",
      "name": "Run Analytics",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [520, 260],
      "parameters": {
        "command": "python3 /srv/ai-software-factory/agents/revenue_system.py analytics"
      }
    }
  ],
  "connections": {
    "Every 6h": { "main": [[{ "node": "Run Analytics", "type": "main", "index": 0 }]] }
  },
  "active": false,
  "settings": { "executionOrder": "v1" }
}
