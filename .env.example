# Telegram bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# OpenRouter
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=openai/gpt-4o-mini

# n8n
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change_me
N8N_ENCRYPTION_KEY=change_this_to_a_long_random_key

# Optional: GitHub automation (recommended)
# Fine-grained or classic PAT with repo contents write permission
GITHUB_PAT=your_github_pat
# owner/repo format
GITHUB_REPO=steev2058/ai-software-factory

# Dashboard Basic Auth (used by nginx proxy + app middleware)
DASHBOARD_USER=admin
DASHBOARD_PASS=change_me_strong_password

# n8n execution webhook (used by dashboard project actions)
N8N_WEBHOOK_URL=http://asf-n8n:5678/webhook/run-project
